{
  "version": 2,
  "name": "estatistica",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/estatistica" }
    }
  ],
  "routes": [
    { "src": "api/insert-results", "dest": "api/insert-results.js" },
    { "src": "api/get-results", "dest": "api/get-results.js" },
    { "src": "/teste", "dest": "teste.js" },
    { "src": "teste", "dest": "/teste.js" },
    { "src": "/teste", "dest": "/src/teste.js" },
    { "src": "/(.*)", "dest": "/$1" },
    { "src": "/", "dest": "/index.html" }
  ]
}
