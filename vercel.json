{
    "version": 2,
    "name": "estatistica",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist/estatistica" }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/$1" },
      { "src": "/", "dest": "/index.html" }
    ]
  }
  